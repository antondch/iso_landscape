package isoCore
{
public class IsoPoint
{
    public var x:Number;
    public var y:Number;
    public var z:Number;

    public function IsoPoint(x:Number = 0, y:Number = 0, z:Number = 0)
    {
        this.x = x;
        this.y = y;
        this.z = z;
    }
}
}
